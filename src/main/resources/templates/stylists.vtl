<h1>All Stylists</h1>
<ul class="jumbotron">
  #foreach ($stylist in $stylists)
  <center><h2><a href="/stylists/$stylist.getId()">$stylist.getName()</a></h2></center>
  <div class="stylist" style="text-align:center;">
    <img class="img-circle" src="$stylist.getImage()" alt="Stylist Picture" width="250" height="146">
  </div>
  #end
</ul>
<a href="/stylist/new">Add New Stylist</a>
